{"ast":null,"code":"import fetch from \"isomorphic-fetch\";\nimport cookie from \"js-cookie\";\nimport { API } from \"../config\";\nimport Router from \"next/router\";\nexport var handleResponse = function handleResponse(response) {\n  if (response.status === 401) {\n    signout(function () {\n      Router.push({\n        pathname: \"/signin\",\n        query: {\n          message: \"Your session is expired. Please signin\"\n        }\n      });\n    });\n  }\n};\nexport var preSignup = function preSignup(user) {\n  return fetch(\"\".concat(API, \"/pre-signup\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var signup = function signup(user) {\n  return fetch(\"\".concat(API, \"/signup\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var signin = function signin(user) {\n  return fetch(\"\".concat(API, \"/signin\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var signout = function signout(next) {\n  removeCookie(\"token\");\n  removeLocalStorage(\"user\");\n  next();\n  return fetch(\"\".concat(API, \"/signout\"), {\n    method: \"GET\"\n  }).then(function (response) {\n    console.log(\"signout success\");\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n}; // set cookie\n\nexport var setCookie = function setCookie(key, value) {\n  if (true) {\n    cookie.set(key, value, {\n      expires: 1\n    });\n  }\n};\nexport var removeCookie = function removeCookie(key) {\n  if (true) {\n    cookie.remove(key, {\n      expires: 1\n    });\n  }\n}; // get cookie\n\nexport var getCookie = function getCookie(key) {\n  if (true) {\n    return cookie.get(key);\n  }\n}; // localstorage\n\nexport var setLocalStorage = function setLocalStorage(key, value) {\n  if (true) {\n    localStorage.setItem(key, JSON.stringify(value));\n  }\n};\nexport var removeLocalStorage = function removeLocalStorage(key) {\n  if (true) {\n    localStorage.removeItem(key);\n  }\n}; // autheticate user by pass data to cookie and localstorage\n\nexport var authenticate = function authenticate(data, next) {\n  setCookie(\"token\", data.token);\n  setLocalStorage(\"user\", data.user);\n  next();\n};\nexport var isAuth = function isAuth() {\n  if (true) {\n    var cookieChecked = getCookie(\"token\");\n\n    if (cookieChecked) {\n      if (localStorage.getItem(\"user\")) {\n        return JSON.parse(localStorage.getItem(\"user\"));\n      } else {\n        return false;\n      }\n    }\n  }\n};\nexport var updateUser = function updateUser(user, next) {\n  if (true) {\n    if (localStorage.getItem(\"user\")) {\n      var auth = JSON.parse(localStorage.getItem(\"user\"));\n      auth = user;\n      localStorage.setItem(\"user\", JSON.stringify(auth));\n      next();\n    }\n  }\n};\nexport var forgotPassword = function forgotPassword(email) {\n  return fetch(\"\".concat(API, \"/forgot-password\"), {\n    method: \"PUT\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(email)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var resetPassword = function resetPassword(resetInfo) {\n  return fetch(\"\".concat(API, \"/reset-password\"), {\n    method: \"PUT\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(resetInfo)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var loginWithGoogle = function loginWithGoogle(user) {\n  return fetch(\"\".concat(API, \"/google-login\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};","map":{"version":3,"sources":["/Users/chanchalagorale/Documents/react-mongo-next-project/frontend/actions/auth.js"],"names":["fetch","cookie","API","Router","handleResponse","response","status","signout","push","pathname","query","message","preSignup","user","method","headers","Accept","body","JSON","stringify","then","json","err","console","log","signup","signin","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","isAuth","cookieChecked","getItem","parse","updateUser","auth","forgotPassword","email","resetPassword","resetInfo","loginWithGoogle"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,kBAAlB;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,SAASC,GAAT,QAAoB,WAApB;AACA,OAAOC,MAAP,MAAmB,aAAnB;AAEA,OAAO,IAAMC,cAAc,GAAG,SAAjBA,cAAiB,CAACC,QAAD,EAAc;AAC1C,MAAIA,QAAQ,CAACC,MAAT,KAAoB,GAAxB,EAA6B;AAC3BC,IAAAA,OAAO,CAAC,YAAM;AACZJ,MAAAA,MAAM,CAACK,IAAP,CAAY;AACVC,QAAAA,QAAQ,EAAE,SADA;AAEVC,QAAAA,KAAK,EAAE;AACLC,UAAAA,OAAO,EAAE;AADJ;AAFG,OAAZ;AAMD,KAPM,CAAP;AAQD;AACF,CAXM;AAaP,OAAO,IAAMC,SAAS,GAAG,SAAZA,SAAY,CAACC,IAAD,EAAU;AACjC,SAAOb,KAAK,WAAIE,GAAJ,kBAAsB;AAChCY,IAAAA,MAAM,EAAE,MADwB;AAEhCC,IAAAA,OAAO