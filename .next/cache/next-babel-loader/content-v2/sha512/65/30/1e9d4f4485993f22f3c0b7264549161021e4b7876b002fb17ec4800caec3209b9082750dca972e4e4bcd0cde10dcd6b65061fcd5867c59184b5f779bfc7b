};\nexport const isAuth = () => {\n  if (false) {\n    const cookieChecked = getCookie(\"token\");\n\n    if (cookieChecked) {\n      if (localStorage.getItem(\"user\")) {\n        return JSON.parse(localStorage.getItem(\"user\"));\n      } else {\n        return false;\n      }\n    }\n  }\n};","map":{"version":3,"sources":["/Users/chanchalagorale/Documents/react-mongo-next-project/frontend/actions/auth.js"],"names":["fetch","cookie","API","signup","user","mode","method","headers","Accept","body","JSON","stringify","then","response","data","json","send","catch","err","console","log","signin","signout","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","token","isAuth","cookieChecked","getItem","parse"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,kBAAlB;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,SAASC,GAAT,QAAoB,WAApB;AAEA,OAAO,MAAMC,MAAM,GAAIC,IAAD,IAAU;AAC9B,SAAOJ,KAAK,CACV,kCADU,EAEV;AAAEK,IAAAA,IAAI,EAAE;AAAR,GAFU,EAGV;AACEC,IAAAA,MAAM,EAAE,MADV;AAEEC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB,kBAFT;AAGP,qCAA+B;AAHxB,KAFX;AAOEC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AAPR,GAHU,CAAL,CAaJQ,IAbI,CAaEC,QAAD,IAAc;AACjB,UAAMC,IAAI,GAAGD,QAAQ,CAACE,IAAT,EAAb;AAEA,WAAQF,QAAQ,CAACG,IAAT,EAAR;AACF,GAjBI,EAkBJC,KAlBI,CAkBGC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CAlBX,CAAP;AAmBD,CApBM;AAsBP,OAAO,MAAMG,MAAM,GAAIjB,IAAD,IAAU;AAC9B,SAAOJ,KAAK,CAAE,GAAEE,GAAI,SAAR,EAAkB;AAC5BI,IAAAA,MAAM,EAAE,MADoB;AAE5BC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB;AAFT,KAFmB;AAM5BC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANsB,GAAlB,CAAL,CAQJQ,IARI,CAQEC,QAAD,IAAc;AAClB,WAAOA,QAAQ,CAACE,IAAT,EAAP;AACD,GAVI,EAWJE,KAXI,CAWGC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CAXX,CAAP;AAYD,CAbM;AAeP,OAAO,MAAMI,OAAO,GAAIC,IAAD,IAAU;AAC/BC,EAAAA,YAAY,CAAC,OAAD,CAAZ;AACAC,EAAAA,kBAAkB,CAAC,MAAD,CAAlB;AACAF,EAAAA,IAAI;AAEJ,SAAOvB,KAAK,CAAE,GAAEE,GAAI,UAAR,EAAmB;AAC7BI,I